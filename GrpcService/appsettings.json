        "OauthAddress": "https://test-xrspace.eu.auth0.com/oauth/token",
        "grant_type": "password",
        "audience": "https://accountserver.xrspace.io",
        "scope": "openid",

        "client_id": "OAUTH_CLIENT_ID",
        "client_secret": "OAUTH_CLIENT_SECRET",

        "username": "USER_EMAIL",
        "password": "USER_PASSWORD"
    },
    "MQTT": {
        "Broker": "broker.emqx.io",
